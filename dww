[32m+[m[32m- "bin/knapsack_pro_rspec"[m
[32m+[m[32menv:[m
[32m+[m[32m  global:[m
[32m+[m[32m  - KNAPSACK_PRO_FIXED_QUEUE_SPLIT=true[m
[32m+[m[32m  - KNAPSACK_PRO_LOG_LEVEL=info[m
[32m+[m[32m  - KNAPSACK_PRO_CI_NODE_TOTAL=2[m
[32m+[m[32m  matrix:[m
[32m+[m[32m  - KNAPSACK_PRO_CI_NODE_INDEX=0[m
[32m+[m[32m  - KNAPSACK_PRO_CI_NODE_INDEX=1[m
[32m+[m[32mnotifications:[m
[32m+[m[32m  slack:[m
[32m+[m[32m    secure: [m
[32m+[m[32m    on_failure: always[m
[32m+[m[32m    on_success: change[m
