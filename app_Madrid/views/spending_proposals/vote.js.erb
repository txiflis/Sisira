$("#<%= dom_id(@spending_proposal) %>_votes").html('<%= j render("spending_proposals/votes", spending_proposal: @spending_proposal, vote_url: vote_spending_proposal_path(@spending_proposal, value: "yes")) %>');
$("#counters").html('<%= j render("spending_proposals/counters") %>');
<% if first_time_voting_spending_proposal_for_district?(@spending_proposal) ||
      last_available_vote_on_spending_proposal?(@spending_proposal) ||
      has_accepted_delegation_alert? %>
  Turbolinks.visit(location.toString());
<% end %>
